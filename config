NGX_ADDON_DEPS="$NGX_ADDON_DEPS $ngx_addon_dir/module.h $ngx_addon_dir/bridge.h $ngx_addon_dir/helpers.h $ngx_addon_dir/filters.h $ngx_addon_dir/required.h"
CORE_LIBS="$CORE_LIBS -lopencv_core -lopencv_imgproc -lopencv_highgui -lwebp"